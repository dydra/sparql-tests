(prefix ((dc: <http://purl.org/dc/elements/1.1/>)
         (bench: <http://localhost/vocabulary/bench/>)
         (foaf: <http://xmlns.com/foaf/0.1/>)
         (rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
         (swrc: <http://swrc.ontoware.org/ontology#>)
         (dcterms: <http://purl.org/dc/terms/>))
  (distinct
    (project (?name1 ?name2)
      (filter (< ?name1 ?name2)
        (bgp
          (triple ?article1 rdf:type bench:Article)
          (triple ?article2 rdf:type bench:Article)
          (triple ?article1 dc:creator ?author1)
          (triple ?author1 foaf:name ?name1)
          (triple ?article2 dc:creator ?author2)
          (triple ?author2 foaf:name ?name2)
          (triple ?article1 swrc:journal ?journal)
          (triple ?article2 swrc:journal ?journal)
        )))))
