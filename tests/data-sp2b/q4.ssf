(select (?name1 ?name2)
  (distinct
    (project (?name1 ?name2)
      (filter (< ?name1 ?name2)
        (bgp
          (triple ?article1 <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://localhost/vocabulary/bench/Article>)
          (triple ?article2 <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://localhost/vocabulary/bench/Article>)
          (triple ?article1 <http://purl.org/dc/elements/1.1/creator> ?author1)
          (triple ?author1 <http://xmlns.com/foaf/0.1/name> ?name1)
          (triple ?article2 <http://purl.org/dc/elements/1.1/creator> ?author2)
          (triple ?author2 <http://xmlns.com/foaf/0.1/name> ?name2)
          (triple ?article1 <http://swrc.ontoware.org/ontology#journal> ?journal)
          (triple ?article2 <http://swrc.ontoware.org/ontology#journal> ?journal)
        )))))
