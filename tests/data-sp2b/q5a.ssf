(select (?person ?name)
  (distinct
    (project (?person ?name)
      (filter (= ?name ?name2)
        (bgp
          (triple ?article <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://localhost/vocabulary/bench/Article>)
          (triple ?article <http://purl.org/dc/elements/1.1/creator> ?person)
          (triple ?inproc <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://localhost/vocabulary/bench/Inproceedings>)
          (triple ?inproc <http://purl.org/dc/elements/1.1/creator> ?person2)
          (triple ?person <http://xmlns.com/foaf/0.1/name> ?name)
          (triple ?person2 <http://xmlns.com/foaf/0.1/name> ?name2)
        )))))
