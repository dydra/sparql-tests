(select (?mbox ?name)
        (project (?mbox ?name)
                 (union
                  (bgp (triple ?x <http://xmlns.com/foaf/0.1/mbox> ?mbox))
                  (bgp
                   (triple ?x <http://xmlns.com/foaf/0.1/mbox> ?mbox)
                   (triple ?x <http://xmlns.com/foaf/0.1/name> ?name)
                   ))))
